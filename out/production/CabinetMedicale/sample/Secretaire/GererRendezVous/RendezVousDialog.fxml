        <Label text="Date " GridPane.columnIndex="0" GridPane.rowIndex="2" />
        <DatePicker fx:id="dateChamp" GridPane.columnIndex="1" GridPane.rowIndex="2" />

        <Label text="Heure" GridPane.columnIndex="0" GridPane.rowIndex="3" />
        <ComboBox GridPane.columnIndex="1" GridPane.rowIndex="3" fx:id="heure">
