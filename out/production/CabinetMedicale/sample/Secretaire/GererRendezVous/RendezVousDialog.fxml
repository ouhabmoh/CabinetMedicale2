                    <String fx:value="14" />
                    <String fx:value="15" />
                    <String fx:value="16" />
                    <String fx:value="17" />
                    <String fx:value="18" />
                    <String fx:value="19" />
                    <String fx:value="20" />
                    <String fx:value="21" />
                    <String fx:value="22" />
                    <String fx:value="23" />



                </FXCollections>
            </items>
            <value>
                <String fx:value="08" />
            </value>
        </ComboBox>

        <ComboBox GridPane.columnIndex="1" GridPane.rowIndex="3" fx:id="min">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="00" />
